{
  "name": "CAPP",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap/cds": "^8",
    "@sap/hana-client": "^2.24.24",
    "@sap/xssec": "^4.7.0",
    "express": "^4",
    "passport": "^0.7.0"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.10.0",
    "@cap-js/sqlite": "^1"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    
  "requires": {
      "db": {
        "kind": "hana"
      },
      "uaa": {
        "kind": "xsuaa"
      },
      "auth": {
        "[development]": {
          "strategy": "mocked",
          "users": {
            "guru": {
              "roles": [
                "Viewer"
              ],
              "userAttributes": {
                "BankName": [
                  "My Bank of San Francisco"
                ]
              }
            },
            "somu": {
              "roles": [
                "Viewer",
                "Admin"
              ],
              "userAttributes": {
                "BankName": [
                  "My Bank of San Francisco"
                ]
              }
            }
          }
        },
        "[production]": {
          "strategy": "JWT"
        }
      }
    },
    "sql": {
      "native_hana_associations": false
    }
  }
}
